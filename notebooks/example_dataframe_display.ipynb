       "<body>\n",
       "\n",
       "\n",
       "            <table id=\"mainTable\" class=\"display\" style=\"width:100%\">\n",
       "                <thead><tr><th>id</th><th>name</th></tr></thead>\n",
       "                <tbody>\n",
       "                    <tr><td>1</td><td>Alice</td></tr>\n",
